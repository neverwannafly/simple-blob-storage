servers=`mysql -s -N -uroot -p10p17fs0008 -e "SELECT s.id from rpc_file_system_development.servers as s"`

for server in $servers
do
  osascript 2>/dev/null <<EOF
    tell application "System Events"
      tell process "Terminal" to keystroke "t" using command down
    end
    tell application "Terminal"
      activate
      do script with command "cd \"$PWD\"; python rpc-server/server.py $server; $*" in window 1
    end tell
EOF
done